# MySQL Client Configuration for Root User
# This file allows root user to connect to MySQL without entering password
# Generated by Ansible - Do not edit manually

[mysql]
user = root
password = {{ mysql_root_password }}

[mysqladmin]
user = root
password = {{ mysql_root_password }}

[client]
user = root
password = {{ mysql_root_password }}

[mysqldump]
user = root
password = {{ mysql_root_password }}
